{
  "name": "logue",
  "description": "Simplest way to test your interactive command-line app.",
  "version": "0.0.0",
  "author": "Yasuaki Uechi <y@uechi.io>",
  "scripts": {
    "build": "npm run tsup -- --minify --dts",
    "clean": "shx rm -rf lib",
    "dev": "npm run tsup -- --watch",
    "pretest": "npm run build",
    "test": "jest",
    "tsup": "tsup src/index.ts -d lib"
  },
  "types": "lib/index.d.ts",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "dependencies": {
    "cross-spawn": "^7.0.3",
    "debug": "^4.1.1",
    "eventemitter3": "^4.0.7"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^1.1.4",
    "@types/cross-spawn": "^6.0.2",
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.2",
    "jest": "^26.4.0",
    "release-it": "^13.6.9",
    "shx": "^0.3.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "tsup": "^3.4.3",
    "typescript": "^4.0.2"
  },
  "homepage": "https://github.com/uetchy/logue",
  "repository": {
    "type": "git",
    "url": "https://github.com/uetchy/logue.git"
  },
  "bugs": {
    "url": "https://github.com/uetchy/logue/issues"
  },
  "license": "Apache-2.0",
  "keywords": [
    "cli",
    "interactive",
    "logue",
    "test"
  ]
}
